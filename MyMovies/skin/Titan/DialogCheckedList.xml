        <textureHeight>56</textureHeight>
        <keepaspectratio>no</keepaspectratio>
        <font2>font10</font2>
        <font3>font10</font3>
        <onleft>2</onleft>
        <onright>10</onright>
        <onup>10</onup>
        <ondown>10</ondown>

        <textcolor>FFFFFFFF</textcolor>                   <!--Colour of text when highlighted-->
        <textcolor2>FFFFFFFF</textcolor2>
        <textcolorNoFocus>00000000</textcolorNoFocus>     <!--Does not appear to be able to be set black-->
