      <control>
        <type>group</type>
        <layout>StackLayout</layout>
        <description>group element</description>
        <posX>650</posX>
        <posY>425</posY>
        <control>
          <description>OK</description>
          <type>button</type>
          <id>2</id>
          <width>#button.width</width>
